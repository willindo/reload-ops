{
  "prisma": {
    "schema": "apps/backend/prisma/schema.prisma",
    "seed": "ts-node --transpile-only apps/backend/prisma/seed.ts"
  },
  "scripts": {
    "switch:local": "./switch-env.sh local",
    "switch:docker": "./switch-env.sh docker",
    "switch:prod": "./switch-env.sh production",
    "dev": "npm run switch:local && npm run generate && npm run seed && cd apps/backend && npm run start:dev",
    "docker": "npm run switch:docker && docker-compose up --build",
    "generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "migrate": "npx prisma migrate dev --name init",
    "postinstall": "npx prisma generate"
  },
  "devDependencies": {
    "@types/next-auth": "^3.13.0",
    "@types/node": "^24.1.0",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "next-auth": "^4.24.11",
    "typescript": "^5.8.3"
  }
}
